<P>BOOL CSXButton::SetIcon( UINT nID, int nWidth, int nHeight )</P>
<P>{</P>
<P>// GS980730 added AfxFindResourceHandle to make MFC find resources in =
DLLs=20
too...</P>
<P>HINSTANCE hInstResource =3D =
AfxFindResourceHandle(MAKEINTRESOURCE(nID),</P>
<P>RT_GROUP_ICON);</P>
<P>m_hIcon =3D (HICON)::LoadImage(hInstResource, </P>
<P>MAKEINTRESOURCE(nID), </P>
<P>IMAGE_ICON, nWidth, nHeight, 0);</P>
<P></P>
<P>if( m_hIcon =3D=3D 0 )</P>
<P>return FALSE;</P>
<P></P>
<P>m_nImageWidth =3D nWidth;</P>
<P>m_nImageHeight =3D nHeight;</P>
<P></P>
<P>m_hBitmap =3D 0;</P>
<P></P>
<P>Redraw();</P>
<P></P>
<P>return TRUE;</P>
<P>}</P>
